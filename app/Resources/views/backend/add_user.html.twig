{% extends 'backend.html.twig' %}
{% block body %}
  <h1>Adding new users</h1>

  {{ form_start(form) }}

    {{ form_errors(form) }}

    {{ form_row(form.login) }}
    {{ form_row(form.password) }}
    {{ form_row(form.email) }}
    {{ form_row(form.first_name) }}
    {{ form_row(form.last_name) }}
    {{ form_row(form.description) }}
    {{ form_row(form.karma) }}
    {{ form_row(form.role) }}
    {{ form_row(form.add) }}
  {{ form_end(form) }}

{% endblock %}